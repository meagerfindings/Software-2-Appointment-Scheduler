<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" maxHeight="200" maxWidth="350.0" minHeight="200" minWidth="350.0" prefHeight="200" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.LoginPrompt">
   <children>
      <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="1.0" layoutY="14.0" prefHeight="23.0" prefWidth="350.0" text="Appointment Scheduler Login" textAlignment="CENTER" textFill="#0063e5">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Text fx:id="usernameLabel" layoutX="51.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" />
      <TextField fx:id="usernameInput" layoutX="129.0" layoutY="61.0" promptText="username" />
      <Text fx:id="passwordLabel" layoutX="52.0" layoutY="130.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
      <PasswordField fx:id="passwordInput" layoutX="129.0" layoutY="112.0" promptText="password" />
      <Button fx:id="submitButton" defaultButton="true" layoutX="139.0" layoutY="159.0" mnemonicParsing="false" onAction="#submitButtonClicked" text="Submit" />
      <Button fx:id="cancelButton" cancelButton="true" layoutX="210.0" layoutY="159.0" mnemonicParsing="false" onAction="#exitButtonClicked" text="Cancel" />
   </children>
</AnchorPane>
